{
  "$schema": "https://gofastmcp.com/schemas/fastmcp_config/v1.json",
  "name": "riversunlimited-google-workspace",
  "version": "1.0.0",
  "description": "Comprehensive MCP server for Google Workspace integration - 72+ tools across Gmail, Drive, Docs, Sheets, Slides, Calendar, Forms, Chat, and Photos",
  "repository": "https://github.com/dipseth/google_workspace_fastmcp2",
  "license": "Apache-2.0",
  "entrypoint": {
    "file": "server.py",
    "object": "mcp"
  },
  "environment": {
    "python": ">=3.11",
    "dependencies": [
      "fastmcp>=2.14.3",
      "google-api-python-client>=2.168.0",
      "google-auth-httplib2>=0.2.0",
      "google-auth-oauthlib>=1.2.2",
      "google-auth",
      "httpx>=0.28.1",
      "aiohttp>=3.8.0",
      "aiofiles>=23.0.0",
      "pydantic>=2.0.0",
      "python-dotenv>=1.0.0",
      "python-card-framework>=2.2.5",
      "qdrant-client>=1.7.0",
      "sentence-transformers>=2.2.0",
      "numpy>=1.24.0",
      "PyJWT>=2.8.0",
      "fastapi>=0.128.0",
      "rich>=13.0.0",
      "colorama>=0.4.6"
    ]
  },
  "tools": {
    "count": "72+",
    "services": [
      "gmail",
      "drive",
      "docs",
      "sheets",
      "slides",
      "calendar",
      "forms",
      "chat",
      "photos"
    ]
  }
}
