# Google OAuth Configuration
# Option 1: Use a Google OAuth JSON file (RECOMMENDED)
# Download from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_CLIENT_SECRETS_FILE=/path/to/your/client_secret.json

# Option 2: Or use individual environment variables
# GOOGLE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
# GOOGLE_CLIENT_SECRET=your_client_secret_here

# OAuth Callback Configuration
OAUTH_REDIRECT_URI=http://localhost:8000/oauth/callback

# Server Configuration
SERVER_PORT=8000
SERVER_HOST=localhost
SERVER_NAME=Google Drive Upload Server

# Storage Configuration
CREDENTIALS_DIR=./credentials

# Logging Configuration
LOG_LEVEL=INFO

# Security Configuration
SESSION_TIMEOUT_MINUTES=60


QDRANT_URL=http://localhost:6333
QDRANT_KEY=NONE